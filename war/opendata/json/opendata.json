      </a>\n        <div class=\"pull-right\" style=\"margin:.5em 1em\">\n            <small>{{\"actualDate\" | translate}}:</small> <i class=\"fa fa-calendar\"></i> {{doc.actualDate.slice(0,10) | date}}\n        </div>\n    </div>\n    <div class=\"clearfix\" style=\"margin-bottom:1em\">\n        <button class=\"btn btn-default btn-xs\"\n            data-trigger=\"focus\"\n            data-placement=\"bottom\"\n            data-toggle=\"popover\"\n            data-container=\"body\"\n            data-html=\"true\"\n            :data-content=\"doc.personas | list\"\n            :disabled=\"ITA.util.castArray(doc.personas).length === 0\">\n                <i class=\"fa fa-users\"></i> {{doc.personas | length}} {{\"personas\" | translate}}\n        </button>\n        &nbsp;\n        <button class=\"btn btn-default btn-xs\"\n            data-trigger=\"focus\"\n            data-placement=\"bottom\"\n            data-toggle=\"popover\"\n            data-container=\"body\"\n            data-html=\"true\"\n            :data-content=\"doc.organizaciones | list\"\n            :disabled=\"ITA.util.castArray(doc.organizaciones).length === 0\">\n                <i class=\"fa fa-university\"></i> {{doc.organizaciones | length}} {{\"organizaciones\" | translate}}\n        </button>\n        &nbsp;\n        <button class=\"btn btn-default btn-xs\"\n            data-trigger=\"focus\"\n            data-placement=\"bottom\"\n            data-toggle=\"popover\"\n            data-container=\"body\"\n            data-html=\"true\"\n            :data-content=\"doc.localizaciones | list\"\n            :disabled=\"ITA.util.castArray(doc.localizaciones).length === 0\">\n                <i class=\"fa fa-location-arrow\"></i> {{doc.localizaciones | length}} {{\"localizaciones\" | translate}}\n        </button>\n    </div>\n    <div style=\"height:250px;overflow:auto;\">\n        <h2 v-if=\"doc.titulo\" style=\"font-size:1.1em;margin-top:0\">{{doc.titulo || \"\"}}</h2>\n        <p v-if=\"highlight(doc.numero, 'texto') !== ''\" v-html=\"highlight(doc.numero, 'texto')\"></p>\n        <p v-else>{{doc.textoResumido}}</p>\n    </div>\n    <div class=\"clearfix\">\n       <button class=\"btn btn-link pull-right scroll-top\">\n           <i class=\"fa fa-arrow-up scroll-top\"></i> {{\"scrollTop\" | translate}}\n       </button>\n    </div>\n</div>\n</div>\n</div>","label":{"personas":{"es":"Personas"},"localizaciones":{"es":"Localizaciones"},"organizaciones":{"es":"Organizaciones"},"actualDate":{"es":"Última actualización"},"scrollTop":{"es":"ir Arriba"}}}}]},{"mode":"solr_pagination","value":{"size":"pagination-sm pull-right","first":"<i class='fa fa-angle-double-left'></i>","last":"<i class='fa fa-angle-double-right'></i>","previous":"<i class='fa fa-angle-left'></i>","next":"<i class='fa fa-angle-right'></i>"}}]}]